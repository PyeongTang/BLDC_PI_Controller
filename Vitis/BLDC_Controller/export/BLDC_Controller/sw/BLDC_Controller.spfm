<?xml version="1.0" encoding="UTF-8"?>
<sdx:platform sdx:vendor="xilinx.com"
              sdx:library="sdx"
              sdx:name="BLDC_Controller"
              sdx:version="1.0"
              sdx:schemaVersion="1.0"
              xmlns:sdx="http://www.xilinx.com/sdx">
    <sdx:description>
BLDC_Controller
    </sdx:description>
    <sdx:systemConfigurations sdx:defaultConfiguration="BLDC_Controller">
        <sdx:configuration sdx:name="BLDC_Controller"
                           sdx:displayName="BLDC_Controller"
                           sdx:defaultProcessorGroup="standalone_domain"
                           sdx:runtimes="cpp">
            <sdx:description>BLDC_Controller</sdx:description>
            <sdx:bootImages sdx:default="standard">
                <sdx:image sdx:name="standard"
                            sdx:bif="BLDC_Controller/boot/BLDC_Controller.bif"
                            sdx:readme="BLDC_Controller/boot/generic.readme"
                            />
            </sdx:bootImages>
            <sdx:processorGroup sdx:name="standalone_domain"
                                sdx:displayName="standalone on microblaze_0"
                                sdx:cpuType="microblaze_0"
                                sdx:cpuInstance="microblaze_0">
                <sdx:os sdx:name="standalone"
                        sdx:displayName="standalone"
                        sdx:bspLibraryPaths="BLDC_Controller/standalone_domain/bsplib/lib"
                        sdx:bspIncludePaths="BLDC_Controller/standalone_domain/bspinclude/include"
                        sdx:bspConfig="BLDC_Controller/standalone_domain/system.mss"
                />
            </sdx:processorGroup>
        </sdx:configuration>
    </sdx:systemConfigurations>
</sdx:platform>
